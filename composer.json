{
    "name": "fanamurov/larrock-core",
    "description": "Core components for LarrockCMS",
    "type": "library",
    "keywords": ["larrock", "larrockCMS", "laravel", "laravel5", "laravel55", "laravel-admin", 
        "cms", "php7", "larrock-core", "larrock-menu", "larrock-users", "larrock-pages", 
        "larrock-blocks", "larrock-contact", "larrock-admin-seo", "larrock-admin-search"],
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "5.6.*",
        "proengsoft/laravel-jsvalidation": "^2.0",
        "davejamesmiller/laravel-breadcrumbs": "5.*",
        "nicolaslopezj/searchable": "^1.9",
        "spatie/laravel-medialibrary": "^6.0.0",
        "fanamurov/mdash": "^1.0"
    },
    "require-dev": {
        "itsgoingd/clockwork": "^2.2",
        "barryvdh/laravel-ide-helper": "~2.4",
        "phpunit/phpunit" : "^6.2|^7.0",
        "mockery/mockery": "^1.0.0",
        "orchestra/testbench": "~3.5.0|~3.6.0",
        "doctrine/dbal": "^2.5.2",
        "php-coveralls/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Larrock\\Core\\": "src",
            "Larrock\\Core\\Models\\": "src/Models",
            "Larrock\\Core\\Requests\\": "src/Requests"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "psr-4": {
            "Larrock\\Core\\Tests\\": "tests"
        }
    },
    "license": "CC-BY-4.0",
    "authors": [
        {
            "name": "Alexandr Fanamurov",
            "email": "fanamurov@ya.ru",
            "homepage": "https://github.com/Fanamurov/",
            "role": "Developer"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Larrock\\Core\\LarrockCoreServiceProvider"
            ]
        }
    }
}